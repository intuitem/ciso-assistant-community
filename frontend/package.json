{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "svelte-kit sync",
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:ci": "vitest run",
		"test:ui": "vitest --ui",
		"test:e2e": "ARGS=\"$npm_config_args\" docker compose -f ./tests/docker-compose.e2e-tests.yml up --force-recreate --build -V",
		"coverage": "vitest run --coverage",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"postinstall": "paraglide-js compile --project ./project.inlang",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "^2.1.0",
		"@inlang/plugin-message-format": "^2.2.0",
		"@playwright/test": "^1.53.1",
		"@skeletonlabs/skeleton": "^3.1.3",
		"@skeletonlabs/skeleton-svelte": "^1.2.3",
		"@storybook/addon-essentials": "^8.6.14",
		"@storybook/addon-interactions": "^8.6.14",
		"@storybook/addon-links": "^8.6.14",
		"@storybook/blocks": "^8.6.14",
		"@storybook/svelte": "^8.6.14",
		"@storybook/sveltekit": "^8.6.14",
		"@storybook/test": "^8.6.14",
		"@svelte-put/copy": "^3.0.2",
		"@svelte-put/qr": "^1.2.1",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.22.2",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@types/node": "^20.19.1",
		"@vincjo/datatables": "^1.14.10",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-intuitem-sveltekit": "file:plugins/eslint/eslint-plugin-intuitem-sveltekit",
		"eslint-plugin-svelte": "^2.46.1",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.1",
		"prettier-plugin-svelte": "^3.4.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"storybook": "^8.6.14",
		"sv-typewriter": "^4.1.2",
		"svelte": "^5.34.8",
		"svelte-check": "^4.2.2",
		"sveltekit-flash-message": "^2.4.6",
		"sveltekit-rate-limiter": "^0.5.2",
		"sveltekit-superforms": "^2.27.4",
		"tailwindcss": "^4.1.11",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"vite": "^5.4.20",
		"vite-plugin-tailwind-purgecss": "^0.3.5",
		"vitest": "^3.2.4",
		"zod": "^3.25.67"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.7.1",
		"@fortawesome/fontawesome-free": "^6.7.2",
		"@tailwindcss/vite": "^4.1.11",
		"@typescript-eslint/eslint-plugin": "^8.35.0",
		"@typescript-eslint/parser": "^8.35.0",
		"@unovis/svelte": "1.5.2",
		"@unovis/ts": "1.5.2",
		"bits-ui": "^0.22.0",
		"dotenv": "^16.5.0",
		"driver.js": "^1.3.6",
		"echarts": "^6.0.0",
		"eslint-plugin-storybook": "^0.10.2",
		"frappe-gantt": "^1.0.3",
		"globals": "^15.15.0",
		"marked": "^16.1.2",
		"sanitize-html": "^2.17.0",
		"svelte-multiselect": "^11.1.1",
		"svelte-persisted-store": "^0.12.0",
		"wx-svelte-grid": "^2.3.0"
	},
	"packageManager": "pnpm@10.18.1",
	"pnpm": {
		"overrides": {
			"esbuild": "^0.25.5",
			"devalue": "^5.3.2"
		}
	}
}
